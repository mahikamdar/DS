package com.list;

public class MyLinkedList {
Node head;
	public static void main(String[] args) {
		MyLinkedList l = new MyLinkedList();
		l.add(10);
		l.add(20);
		l.add(30);
		l.add(40);
		l.add(50);
		
	}

	private void add(int i) {
		
		Node temp = new Node(i);
		if(head == null)head =temp;
		else {
			Node node =head;
			while(node.next != null)node = node.next;
			node.next = temp;
		}
	}
}
